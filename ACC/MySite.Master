                $('#ContentPlaceHolder1_txtPHDate2').calendarsPicker({ calendar: $.calendars.instance('ummalqura') });
                $('#ContentPlaceHolder1_txtPHDate3').calendarsPicker({ calendar: $.calendars.instance('ummalqura') });
                $('#ContentPlaceHolder1_txtPHDate4').calendarsPicker({ calendar: $.calendars.instance('ummalqura') });
            }
        }

